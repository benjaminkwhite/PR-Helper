{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "github-120.png",
      "default_popup": "popup.html",
      "default_title": "View your Pull Requests"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Keep track of the Pull Requests of your favorite repositories \u003C3",
   "icons": {
      "128": "github-120.png",
      "16": "github-120.png",
      "48": "github-120.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pull Request Notifier",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*.github.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.0"
}
