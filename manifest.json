{
  "name": "PR Helper",
  "version": "1.4",
  "description": "Keep track your PRs",
  "homepage_url": "https://github.com/benjaminkwhite/PR-Helper",
  "manifest_version": 2,
  "icons": {
    "128": "github-120.png",
    "16": "github-120.png",
    "48": "github-120.png"
  },
  "permissions": [
    "tabs",
    "alarms"
  ],
  "optional_permissions": [
    "tabs",
    "notifications"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-2.0.3.min.js",
      "underscore-min.js",
      "dataPull.js"
    ]
  },
  "browser_action": {
    "default_icon": "github-120.png",
    "default_popup": "popup.html",
    "default_title": "PR Helper"
  },
  "content_scripts": [{
    "js": ["addButton.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_end"
  }],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}